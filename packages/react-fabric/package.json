{
  "name": "@kubb/react-fabric",
  "version": "0.7.0",
  "description": "React integration for Kubb, providing JSX runtime support and React component generation capabilities for code generation plugins.",
  "keywords": [
    "react",
    "jsx",
    "component-generation",
    "typescript",
    "code-generator",
    "codegen",
    "plugins",
    "kubb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kubb-labs/fabric.git",
    "directory": "packages/react-fabric"
  },
  "license": "MIT",
  "author": "stijnvanhulle",
  "sideEffects": [
    "./dist/globals.js",
    "./dist/globals.cjs"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./devtools": {
      "require": "./dist/devtools.cjs",
      "import": "./dist/devtools.js"
    },
    "./globals": {
      "require": "./dist/globals.cjs",
      "import": "./dist/globals.js"
    },
    "./jsx-dev-runtime": {
      "require": "./dist/jsx-dev-runtime.cjs",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./jsx-runtime": {
      "require": "./dist/jsx-runtime.cjs",
      "import": "./dist/jsx-runtime.js"
    },
    "./parsers": {
      "require": "./dist/parsers.cjs",
      "import": "./dist/parsers.js"
    },
    "./plugins": {
      "require": "./dist/plugins.cjs",
      "import": "./dist/plugins.js"
    },
    "./types": {
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "typesVersions": {
    "*": {
      "globals": [
        "./dist/globals.d.ts"
      ],
      "devtools": [
        "./dist/devtools.d.ts"
      ],
      "jsx-dev-runtime": [
        "./dist/jsx-runtime.d.ts"
      ],
      "jsx-runtime": [
        "./dist/jsx-runtime.d.ts"
      ],
      "parsers": [
        "./dist/parsers.d.ts"
      ],
      "plugins": [
        "./dist/plugins.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "*.d.ts",
    "*.d.cts",
    "!*.map",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "size-limit": [
    {
      "path": "./dist/*.js",
      "limit": "510 KiB",
      "gzip": true
    }
  ],
  "scripts": {
    "build": "tsdown && size-limit",
    "start": "tsdown --watch ./src",
    "clean": "npx rimraf ./dist",
    "lint": "pnpm biome lint .",
    "lint:fix": "pnpm biome lint --fix --unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check -tag canary",
    "start:devtools": "npx react-devtools",
    "test": "vitest --passWithNoTests",
    "test:devtools": "node ./dist/runner.cjs",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/fabric-core": "workspace:*",
    "dedent": "^1.7.1",
    "execa": "^9.6.1",
    "indent-string": "^5.0.0",
    "natural-orderby": "^5.0.0",
    "react-devtools-core": "6.1.2",
    "signal-exit": "^4.1.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-reconciler": "catalog:",
    "@types/ws": "^8.18.1",
    "react": "catalog:",
    "react-reconciler": "catalog:"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
